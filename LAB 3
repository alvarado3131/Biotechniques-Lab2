# Question: Is petal length different between species?

data(iris)

anovapetal <- aov(Petal.Length ~ Species, data = iris)
summary(anovapetal)

TukeyHSD(anovapetal)



boxplot(Petal.Length ~ Species, data = iris,
        col = c("lightblue","lightgreen","lightpink"),
        main = "Petal length differences",
        ylab = "Petal Length")




